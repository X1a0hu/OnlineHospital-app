d: \"get\",\r\n        url: url,\r\n        headers: {\r\n          Authorization:\r\n            \"Bearer \" + JSON.parse(sessionStorage.getItem(\"token\")),\r\n        },\r\n        params: {\r\n          page: this.currentPage,\r\n          size: this.pageSize,\r\n          doctorID: sessionStorage.getItem(\"userID\"),\r\n        },\r\n      }).then((response) => {\r\n        console.log(response.data.data);\r\n        this.tableData = response.data.data.list;\r\n      });\r\n    },\r\n    onSearch() {\r\n      console.log(\"查询条件:\", this.form);\r\n      this.fetchData();\r\n    },\r\n    onReset() {\r\n      this.form = {\r\n        userID: \"\",\r\n        realName: \"\",\r\n        sex: \"\",\r\n        setMeal: \"\",\r\n        date: \"\",\r\n        archived: false,\r\n      };\r\n    },\r\n\r\n    handleCurrentChange(page) {\r\n      this.currentPage = page;\r\n      this.fetchData();\r\n    },\r\n    editReport(row) {\r\n      this.$router.push({ path: \"/editReport\", query: { id: row.orderid } });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style src=\"@/assets/css/ReportManageSystem.css\" scoped></style>\r\n"],"mappings":";AAkIA,OAAAA,YAAA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,QAAA;MACAC,WAAA;MAEA;MACAC,IAAA;QACAC,MAAA;QAAA;QACAC,QAAA;QAAA;QACAC,GAAA;MACA;MACAC,IAAA;MAAA;MACAC,KAAA;MAAA;MACA;;MAEAC,cAAA;MAAA;MACAC,oBAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,SAAAF,GAAA;QACA,KAAAT,WAAA,GAAAS,GAAA;QACA,KAAAG,SAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,aAAA;MACA,IAAAC,GAAA;MACA,KAAAC,MAAA;QACAC,MAAA;QACAF,GAAA,EAAAA,GAAA;QACAG,OAAA;UACAC,aAAA,EACA,YAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;QACA;QACAC,MAAA;UACAC,IAAA,OAAAzB,WAAA;UACA0B,IAAA,OAAA3B,QAAA;UACA4B,QAAA,EAAAL,cAAA,CAAAC,OAAA;QACA;MACA,GAAAK,IAAA,CAAAC,QAAA;QACAnB,OAAA,CAAAC,GAAA,CAAAkB,QAAA,CAAAjC,IAAA,CAAAA,IAAA;QACA,KAAAC,SAAA,GAAAgC,QAAA,CAAAjC,IAAA,CAAAA,IAAA,CAAAkC,IAAA;MACA;IACA;IACAC,SAAA;MACArB,OAAA,CAAAC,GAAA,eAAAqB,IAAA;MACA,KAAApB,SAAA;IACA;