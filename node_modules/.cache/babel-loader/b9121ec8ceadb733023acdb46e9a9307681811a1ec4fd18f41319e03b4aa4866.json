{"ast":null,"code":"export default {\n  data() {\n    return {\n      form: {\n        phonenum: '',\n        name: '',\n        sex: '',\n        setmeal: '',\n        date: '',\n        archived: false\n      },\n      urineRoutine: {\n        leukocyte: '',\n        protein: '',\n        glucose: '',\n        ketone: ''\n      },\n      normalRange: {\n        leukocyte: [0, 10],\n        // 假设正常范围是0-10\n        protein: [0, 0.2],\n        // 假设正常范围是0-0.2\n        glucose: [0, 0.8],\n        // 假设正常范围是0-0.8\n        ketone: [0, 0.5] // 假设正常范围是0-0.5\n      }\n    };\n  },\n  methods: {\n    onSearch() {\n      console.log('查询条件:', this.form);\n      this.fetchData();\n    },\n    onReset() {\n      this.form = {\n        phonenum: '',\n        name: '',\n        sex: '',\n        setmeal: '',\n        date: '',\n        archived: false\n      };\n    },\n    fetchData() {\n      // 模拟获取数据\n      console.log('Fetching data...');\n    },\n    isNormal(value, type) {\n      const range = this.normalRange[type];\n      return value >= range[0] && value <= range[1];\n    },\n    saveUrineRoutine() {\n      console.log('尿常规数据已保存:', this.urineRoutine);\n      // 在这里添加保存数据的逻辑\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["data","form","phonenum","name","sex","setmeal","date","archived","urineRoutine","leukocyte","protein","glucose","ketone","normalRange","methods","onSearch","console","log","fetchData","onReset","isNormal","value","type","range","saveUrineRoutine","mounted"],"sources":["src/views/EditReport.vue"],"sourcesContent":["<template>\r\n    <div class=\"Wrapper\">\r\n      <div class=\"health-check-report\">\r\n        <el-row>\r\n          <!-- 左侧部分 -->\r\n          <el-col :span=\"6\">\r\n            <el-header class=\"header\">\r\n              <div class=\"header-title\">体检报告查询</div>\r\n            </el-header>\r\n            <el-form :model=\"form\" label-width=\"100px\" class=\"demo-form-inline\">\r\n              <el-form-item label=\"手机号码\">\r\n                <el-input v-model=\"form.phonenum\" placeholder=\"请输入手机号码\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"姓名\">\r\n                <el-input v-model=\"form.name\" placeholder=\"请输入姓名\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"性别\">\r\n                <el-select v-model=\"form.sex\" placeholder=\"请选择性别\">\r\n                  <el-option label=\"男\" value=\"male\"></el-option>\r\n                  <el-option label=\"女\" value=\"female\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item label=\"套餐类型\">\r\n                <el-select v-model=\"form.setmeal\" placeholder=\"请选择套餐类型\">\r\n                  <el-option label=\"基础套餐\" value=\"basic\"></el-option>\r\n                  <el-option label=\"高级套餐\" value=\"advanced\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item label=\"体检日期\">\r\n                <el-date-picker v-model=\"form.date\" type=\"date\" placeholder=\"请选择体检日期\"></el-date-picker>\r\n              </el-form-item>\r\n              <el-form-item label=\"是否归档\">\r\n                <el-switch v-model=\"form.archived\"></el-switch>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-button type=\"primary\" @click=\"onSearch\">查询</el-button>\r\n                <el-button @click=\"onReset\">重置</el-button>\r\n              </el-form-item>\r\n            </el-form>\r\n          </el-col>\r\n          \r\n          <!-- 右侧部分 -->\r\n          <el-col :span=\"18\">\r\n            <div class=\"report-section\">\r\n              <h3>尿常规</h3>\r\n              <el-form :model=\"urineRoutine\" label-width=\"150px\">\r\n                <el-row>\r\n                  <el-col :span=\"12\">\r\n                    <el-form-item label=\"尿白细胞\">\r\n                      <template v-if=\"!isNormal(urineRoutine.leukocyte, 'leukocyte')\">\r\n                        <span class=\"abnormal\">异常</span>\r\n                      </template>\r\n                      <el-input v-model=\"urineRoutine.leukocyte\" placeholder=\"请输入数值\" style=\"width: 100px;\"></el-input>\r\n                      <span>/ul 正常范围：{{ normalRange.leukocyte[0] }}-{{ normalRange.leukocyte[1] }}</span>\r\n                    </el-form-item>\r\n                  </el-col>\r\n                  <el-col :span=\"12\">\r\n                    <el-form-item label=\"尿蛋白\">\r\n                      <template v-if=\"!isNormal(urineRoutine.protein, 'protein')\">\r\n                        <span class=\"abnormal\">异常</span>\r\n                      </template>\r\n                      <el-input v-model=\"urineRoutine.protein\" placeholder=\"请输入数值\" style=\"width: 100px;\"></el-input>\r\n                      <span>g/L 正常范围：{{ normalRange.protein[0] }}-{{ normalRange.protein[1] }}</span>\r\n                    </el-form-item>\r\n                  </el-col>\r\n                </el-row>\r\n                <!-- 添加其他尿常规小项目 -->\r\n                <el-row>\r\n                  <el-col :span=\"12\">\r\n                    <el-form-item label=\"尿糖\">\r\n                      <template v-if=\"!isNormal(urineRoutine.glucose, 'glucose')\">\r\n                        <span class=\"abnormal\">异常</span>\r\n                      </template>\r\n                      <el-input v-model=\"urineRoutine.glucose\" placeholder=\"请输入数值\" style=\"width: 100px;\"></el-input>\r\n                      <span>mmol/L 正常范围：{{ normalRange.glucose[0] }}-{{ normalRange.glucose[1] }}</span>\r\n                    </el-form-item>\r\n                  </el-col>\r\n                  <el-col :span=\"12\">\r\n                    <el-form-item label=\"尿酮体\">\r\n                      <template v-if=\"!isNormal(urineRoutine.ketone, 'ketone')\">\r\n                        <span class=\"abnormal\">异常</span>\r\n                      </template>\r\n                      <el-input v-model=\"urineRoutine.ketone\" placeholder=\"请输入数值\" style=\"width: 100px;\"></el-input>\r\n                      <span>mg/dL 正常范围：{{ normalRange.ketone[0] }}-{{ normalRange.ketone[1] }}</span>\r\n                    </el-form-item>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-form-item>\r\n                  <el-button type=\"primary\" @click=\"saveUrineRoutine\">尿常规项保存</el-button>\r\n                </el-form-item>\r\n              </el-form>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        form: {\r\n          phonenum: '',\r\n          name: '',\r\n          sex: '',\r\n          setmeal: '',\r\n          date: '',\r\n          archived: false\r\n        },\r\n        urineRoutine: {\r\n          leukocyte: '',\r\n          protein: '',\r\n          glucose: '',\r\n          ketone: ''\r\n        },\r\n        normalRange: {\r\n          leukocyte: [0, 10], // 假设正常范围是0-10\r\n          protein: [0, 0.2],   // 假设正常范围是0-0.2\r\n          glucose: [0, 0.8],   // 假设正常范围是0-0.8\r\n          ketone: [0, 0.5]     // 假设正常范围是0-0.5\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      onSearch() {\r\n        console.log('查询条件:', this.form);\r\n        this.fetchData();\r\n      },\r\n      onReset() {\r\n        this.form = {\r\n          phonenum: '',\r\n          name: '',\r\n          sex: '',\r\n          setmeal: '',\r\n          date: '',\r\n          archived: false\r\n        };\r\n      },\r\n      fetchData() {\r\n        // 模拟获取数据\r\n        console.log('Fetching data...');\r\n      },\r\n      isNormal(value, type) {\r\n        const range = this.normalRange[type];\r\n        return value >= range[0] && value <= range[1];\r\n      },\r\n      saveUrineRoutine() {\r\n        console.log('尿常规数据已保存:', this.urineRoutine);\r\n        // 在这里添加保存数据的逻辑\r\n      }\r\n    },\r\n    mounted() {\r\n      this.fetchData();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .abnormal {\r\n    color: red;\r\n    margin-right: 5px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAmGA;EACAA,KAAA;IACA;MACAC,IAAA;QACAC,QAAA;QACAC,IAAA;QACAC,GAAA;QACAC,OAAA;QACAC,IAAA;QACAC,QAAA;MACA;MACAC,YAAA;QACAC,SAAA;QACAC,OAAA;QACAC,OAAA;QACAC,MAAA;MACA;MACAC,WAAA;QACAJ,SAAA;QAAA;QACAC,OAAA;QAAA;QACAC,OAAA;QAAA;QACAC,MAAA;MACA;IACA;EACA;EACAE,OAAA;IACAC,SAAA;MACAC,OAAA,CAAAC,GAAA,eAAAhB,IAAA;MACA,KAAAiB,SAAA;IACA;IACAC,QAAA;MACA,KAAAlB,IAAA;QACAC,QAAA;QACAC,IAAA;QACAC,GAAA;QACAC,OAAA;QACAC,IAAA;QACAC,QAAA;MACA;IACA;IACAW,UAAA;MACA;MACAF,OAAA,CAAAC,GAAA;IACA;IACAG,SAAAC,KAAA,EAAAC,IAAA;MACA,MAAAC,KAAA,QAAAV,WAAA,CAAAS,IAAA;MACA,OAAAD,KAAA,IAAAE,KAAA,OAAAF,KAAA,IAAAE,KAAA;IACA;IACAC,iBAAA;MACAR,OAAA,CAAAC,GAAA,mBAAAT,YAAA;MACA;IACA;EACA;EACAiB,QAAA;IACA,KAAAP,SAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}